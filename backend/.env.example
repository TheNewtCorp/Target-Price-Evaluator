# Environment variables for Target Price Evaluator Backend

# Server Configuration
PORT=3001
NODE_ENV=development

# Playwright Configuration
HEADLESS=false
# Set HEADLESS=true for production deployment

# Performance Settings
MIN_DELAY_MS=1000
MAX_DELAY_MS=3000
TYPING_DELAY_MIN=50
TYPING_DELAY_MAX=150

# Session Management
SESSION_TIMEOUT_MS=300000

# Logging
LOG_LEVEL=info
ENABLE_DEBUG_LOGGING=false

# Production CORS Origins (update with your actual frontend URLs)
FRONTEND_URLS=https://your-frontend-domain.onrender.com,https://target-price-evaluator.netlify.app
