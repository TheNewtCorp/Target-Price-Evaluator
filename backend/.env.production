# Production Environment Variables for Target Price Evaluator

# Server Configuration
PORT=3001
NODE_ENV=production

# Playwright Configuration (Headless for Production)
HEADLESS=true

# Anti-Detection Settings
MIN_DELAY_MS=1500
MAX_DELAY_MS=4000
TYPING_DELAY_MIN=75
TYPING_DELAY_MAX=200

# Session Management
SESSION_TIMEOUT_MS=300000
COOKIE_MAX_AGE=7776000000

# Logging (Production)
LOG_LEVEL=info
ENABLE_DEBUG_LOGGING=false

# Security
DISABLE_SCREENSHOTS=true
CLEANUP_TIMEOUT_MS=10000
