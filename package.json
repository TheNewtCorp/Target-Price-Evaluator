{
  "version": "0.0.0",
  "name": "luxury-deal-finder",
  "author": "zerodot",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=9.0.0 <12.0.0"
  },
  "packageManager": "npm@11.4.1",
  "scripts": {
    "dev": "run-s clean && vite",
    "dev:with-backend": "concurrently \"cd backend && npm run dev\" \"npm run dev\"",
    "build": "tsc -b && vite build",
    "build2": "run-s clean && tsc && vite build && run-s firefox-mv2-build",
    "firefox-mv2-build": "node tools/unstable_mv3ToMv2Build.js",
    "clean": "run-p clean:dist clean:dist-firefox-v2",
    "clean:dist": "rimraf dist",
    "clean:dist-firefox-v2": "rimraf dist-firefox-v2",
    "format": "run-p format:eslint format:prettier format:css format:type",
    "format:css": "stylelint \"src/**/*.{css,scss}\" --fix",
    "format:eslint": "eslint . -c eslint.config.mts --fix",
    "format:prettier": "prettier -w \"**/*.(js|jsx|mjs|cjs|ts|tsx|mts|cts|json|md|yml|html)\" --ignore-path .gitignore -c",
    "format:type": "tsc --noEmit",
    "preinstall": "npx only-allow npm",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') require('child_process').execSync('npx husky install', {stdio: 'inherit'})\" || exit 0",
    "start": "node backend/server.cjs",
    "render-build": "npm install && npm run build",
    "render-start": "node backend/server.cjs",
    "backend:dev": "cd backend && nodemon server.cjs",
    "backend:start": "node backend/server.cjs",
    "netlify:build": "npm run build",
    "deploy:test": "npm run build && npm run backend:start"
  },
